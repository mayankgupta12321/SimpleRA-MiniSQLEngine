TEMP_Q2_1 <- SELECT Stud_sex == 1 FROM Student
TEMP_Q2_2 <- SELECT Stud_age > 16 FROM Student
TEMP_Q2_3 <- CROSS TEMP_Q2_1 TEMP_Q2_2
TEMP_Q2_4 <- SELECT TEMP_Q2_1_Stud_Id == TEMP_Q2_2_Stud_Id FROM TEMP_Q2_3
Q2 <- PROJECT TEMP_Q2_1_Stud_Id, TEMP_Q2_1_Stud_age FROM TEMP_Q2_4
RENAME TEMP_Q2_1_Stud_Id TO Stud_Id FROM Q2
RENAME TEMP_Q2_1_Stud_age TO Stud_age FROM Q2
PRINT Q2
