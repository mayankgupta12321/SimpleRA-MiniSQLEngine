TEMP_Q6_1 <- SELECT Yrs_of_exp <= 4 FROM Advisor
TEMP_Q6_2 <- SELECT Salary > 300000 FROM Advisor
TEMP_Q6_3 <- CROSS TEMP_Q6_1 TEMP_Q6_2
TEMP_Q6_4 <- SELECT TEMP_Q6_1_Adv_Id == TEMP_Q6_2_Adv_Id FROM TEMP_Q6_3
Q6 <- PROJECT TEMP_Q6_1_Adv_Id, TEMP_Q6_1_Yrs_of_exp FROM TEMP_Q6_4
RENAME TEMP_Q6_1_Adv_Id TO adv_id FROM Q6
RENAME TEMP_Q6_1_Yrs_of_exp TO yrs_of_exp FROM Q6
PRINT Q6